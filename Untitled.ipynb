   "source": [
    "def rotate(input1):\n",
    "    input1 = input1.split(\" \")\n",
    "    output = []\n",
    "    for word in input1:\n",
    "        if word == word[::-1]:\n",
    "            output.append(\"$\" + word + \"$\")\n",
    "        else:\n",
    "            output.append(word[::-1])\n",
    "\n",
    "    return \" \".join(output)"
   ]
